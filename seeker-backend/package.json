    "lender": "pnpm run lender:firehose:events & pnpm run lender:index:events",
    "index": "pnpm run issuer & pnpm run lender",
    "generate": "drizzle-kit generate",
    "migrate": "drizzle-kit migrate",
    "clean-db": "rm -rf ./local-store/sqlite/sqlite.db && tsx ./events/clean.ts && rm -rf ./db/migrations && pnpm run generate && pnpm run migrate",
